<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="yongs.temp.mapper.TUserMapper">
    <insert id="insert"> 
        INSERT INTO tuser (id, name) VALUES (#{id}, #{name}) 
    </insert>
    
    <select id="selectList" resultType="yongs.temp.vo.TUserVo">
        SELECT id, name FROM tuser
    </select>
</mapper>